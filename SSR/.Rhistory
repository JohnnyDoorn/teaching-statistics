dummy.2 <- ifelse(nationality == "Belgian", 1, 0)
# Set parameters
b.0 <- 15 # initial value for group 1
b.1 <- 3  # difference between group 1 and 2
b.2 <- 4  # difference between group 1 and 3
b.3 <- 3  # Weight for covariate
# Create error
error <- rnorm(n,0,2)
